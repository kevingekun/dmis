<struts>

	<package name="TYPE" namespace="/Type" extends="struts-default" >
		<action name="*" class="lab.dmis.web.TypeAction" method="{1}">
			<result name="list">/type.jsp</result>
			<result name="delete_success" type="redirect">/Type/listAll?pageNo=${#request.pageNo}</result>
